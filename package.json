{
  "name": "hybrid-ai-workbench",
  "version": "0.1.0",
  "description": "Hybrid AI workstation manager for RTX 5090 - intelligent routing between local models and cloud APIs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "hybrid-ai-workbench": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint"
  },
  "keywords": [
    "gpu",
    "nvidia",
    "rtx-5090",
    "ai",
    "machine-learning",
    "llm",
    "model-router",
    "hybrid-ai",
    "security-hardening",
    "workstation"
  ],
  "author": "RTX 5090 Hybrid AI Research",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "yaml": "^2.3.4",
    "chalk": "^4.1.2",
    "ora": "^5.4.1",
    "inquirer": "^8.2.6"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/jest": "^29.5.11",
    "@types/inquirer": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "husky": "^8.0.3"
  },
  "optionalDependencies": {
    "node-nvidia-smi": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eatexp/bookish-potato.git"
  },
  "bugs": {
    "url": "https://github.com/eatexp/bookish-potato/issues"
  },
  "homepage": "https://github.com/eatexp/bookish-potato#readme"
}
